	<box id="main-bottombar-toolbar-buttons">
		<toolbarbutton label="Detach" insertbefore="main-bottombar-toolbar-buttons-close" class="detach-button" oncommand="uienhancements_bottombar_detach()"/>
	</box>
  
</overlay>
